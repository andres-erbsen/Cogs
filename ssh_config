Protocol 2
Compression yes

ControlMaster auto
ControlPersist yes
ControlPath ~/.ssh/socket-%r@%h:%p

host athena
	Hostname athena.dialup.mit.edu
	User andreser
	GSSAPIAuthentication yes
	GSSAPIDelegateCredentials yes
	ForwardX11 yes

